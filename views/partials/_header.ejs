<title>TinyApp</title>

<% if (!username) { %>
<form method="POST" action="/login">
  <input type="text" id="username" name="username" placeholder="Enter Username">
  <button type="submit" >Login</button>
</form>
<% } else { %>
<form method="POST" action="/logout">
  <%= username %>
  <button type="submit" >Logout</button>
</form>
<% } %>